{
    "name": "jumpy-vscode",
    "displayName": "jumpy-vscode",
    "description": "Jumpy for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:jumpy.toggle"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "colors": [
            {
                "id": "jumpy2.labelBackgroundColor",
                "description": "Background color for Jumpy2 labels.  Overrides dark, light, and highContrast, so theme specific overrides should apply.",
                "defaults": {
                    "dark": "editorGutter.addedBackground",
                    "light": "editorGutter.addedBackground",
                    "highContrast": "editorGutter.addedBackground"
                }
            },
            {
                "id": "jumpy2.labelFontColor",
                "description": "Font color for Jumpy2 labels.  Overrides dark, light, and highContrast, so theme specific overrides should apply.",
                "defaults": {
                    "dark": "foreground",
                    "light": "foreground",
                    "highContrast": "foreground"
                }
            }
        ],
        "configuration": {
            "title": "Jumpy2",
            "properties": {
                "jumpy2.wordPattern": {
                    "type": "string",
                    "default": "([A-Z]+([0-9a-z])*)|[a-z0-9]{2,}",
                    "description": "Regex pattern for Jumpy2 to use in text editors."
                }
            }
        },
        "commands": [
            {
                "command": "jumpy.toggle",
                "title": "Jumpy: Toggle"
            },
            {
                "command": "jumpy.reset",
                "title": "Jumpy: Reset"
            },
            {
                "command": "jumpy.clear",
                "title": "Jumpy: Clear"
            }
        ],
        "keybindings": [
            {
                "command": "jumpy.toggle",
                "key": "shift+enter",
                "when": "editorTextFocus"
            },
            {
                "command": "jumpy.clear",
                "key": "enter",
                "when": "jumpy.jump-mode && editorTextFocus"
            },
            {
                "command": "jumpy.clear",
                "key": "escape",
                "when": "jumpy.jump-mode && editorTextFocus"
            },
            {
                "command": "jumpy.clear",
                "key": "space",
                "when": "jumpy.jump-mode && editorTextFocus"
            },
            {
                "command": "jumpy.a",
                "key": "a",
                "when": "jumpy.jump-mode && editorTextFocus"
            },
