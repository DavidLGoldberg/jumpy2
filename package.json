{
    "name": "jumpy-vscode",
    "displayName": "jumpy-vscode",
    "description": "Jumpy for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:jumpy.toggle"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "jumpy.toggle",
                "title": "Jumpy: Toggle"
            },
            {
                "command": "jumpy.reset",
                "title": "Jumpy: Reset"
            },
            {
                "command": "jumpy.clear",
                "title": "Jumpy: Clear"
            }
        ],
        "keybindings": [
            {
                "command": "jumpy.toggle",
                "key": "shift+enter",
                "mac": "shift+enter"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.38.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "@types/lodash": "4.14.141",
        "lodash": "4.17.15"
    }
}
